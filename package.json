{
  "name": "eprintdiff-extension",
  "version": "0.0.1",
  "description": "Compare two PDF versions from eprint.iacr.org archive",
  "scripts": {
    "postinstall": "copyfiles -u 3 \"node_modules/pdfjs-dist/build/pdf.min.mjs\" \"node_modules/pdfjs-dist/build/pdf.worker.min.mjs\" \"node_modules/pdf-lib/dist/pdf-lib.min.js\" ."
  },
  "dependencies": {
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.4.394"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1"
  }
}
